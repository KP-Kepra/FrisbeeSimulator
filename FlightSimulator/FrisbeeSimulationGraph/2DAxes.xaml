<UserControl x:Class="FrisbeeSimulationGraph.Axes2D"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="300" Height="300">
    <Grid x:Name="m_layout">
        <Line  X1="0.5" Y1="0" X2="0.5" Y2="1" Stretch="Fill" Stroke="Black"/>
        <Line  X1="0" Y1="0.5" X2="1" Y2="0.5" Stretch="Fill" Stroke="Black"/>
    </Grid>
</UserControl>
